n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  padding-left: 5%;\r\n}\r\n#title .title_slide li:nth-child(1) {\r\n  background-image: url(\"../img/main_visual_01.jpg\");\r\n}\r\n#title .title_slide li:nth-child(2) {\r\n  background-image: url(\"../img/main_visual_02.jpg\");\r\n}\r\n#title .title_slide h3 {\r\n  font-size: 60px;\r\n  letter-spacing: 12px;\r\n  font-weight: 200;\r\n  line-height: 1.3;\r\n  color: #333;\r\n  margin-bottom: 30px;\r\n}\r\n#title .title_slide p {\r\n  font-weight: 200;\r\n}\r\n#title .title_slide li a {\r\n  margin-top: 50px;\r\n  color: #007a88;\r\n  position: relative;\r\n  width: 170px;\r\n}\r\n#title .title_slide li a:hover::after {\r\n  transform: scale(1.1);\r\n}\r\n#title .title_slide li a::after {\r\n  content: \"\";\r\n  display: block;\r\n  width: 40px;\r\n  height: 40px;\r\n  background: #007a88;\r\n  border-radius: 50%;\r\n  position: absolute;\r\n  top: calc(50% - 20px);\r\n  right: -40px;\r\n  transition: 0.5s;\r\n}\r\n#title .title_slide li a span {\r\n  display: block;\r\n  width: 50px;\r\n  height: 1px;\r\n  background: #007a88;\r\n  position: absolute;\r\n  top: calc(50% - 0.5px);\r\n  right: 0;\r\n  transition: 0.5s;\r\n}\r\n#title .title_slide li a span::before {\r\n  content: \"→\";\r\n  position: absolute;\r\n  top: -10px;\r\n  right: -17px;\r\n  color: #fff;\r\n  z-index: 2;\r\n}\r\n#title .title_slide li a:hover span {\r\n  animation-name: more;\r\n  animation-duration: 0.5s;\r\n}\r\n@keyframes more {\r\n  30% {\r\n    right: -50px;\r\n  }\r\n  50% {\r\n    opacity: 0;\r\n    right: 20px;\r\n  }\r\n  60%{\r\n    opacity: 1;\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    right: 0;\r\n  }\r\n}\r\n#title .slide_count {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 90%;\r\n  width: 60px;\r\n}\r\n#title .slide_count button {\r\n  background: transparent;\r\n  border: none;\r\n  width: 100%;\r\n  height: 20px;\r\n  position: relative;\r\n  cursor: pointer;\r\n}\r\n#title .slide_count button::before {\r\n  content: \"\";\r\n  display: inline-block;\r\n  width: 100%;\r\n  height: 1px;\r\n  background: #333;\r\n}\r\n#title .slide_count .left::after {\r\n  content: \"←\";\r\n  position: absolute;\r\n  left: 0;\r\n  top: 1px;\r\n  font-size: 18px;\r\n}\r\n#title .slide_count .right::after {\r\n  content: \"→\";\r\n  position: absolute;\r\n  right: 0;\r\n  top: 1px;\r\n  font-size: 18px;\r\n}\r\n#title .slide_btn {\r\n\r\n}\r\n#title .slide_btn li {\r\n\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"86aaacfc7f3b132004278aa9e322291a","cacheData":{"env":{}}}